{
  "name": "agent-mode-engine",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json --incremental",
    "build:clean": "npm run clean && tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --incremental --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "npm run test:unit",
    "test:unit": "node ./scripts/run-unit-tests.mjs",
    "test:integration": "node ./scripts/run-integration-tests.mjs",
    "dev": "tsx src/cli/run-command.ts",
    "start": "node dist/cli/run-command.js",
    "smoke": "node ./scripts/smoke-local.js",
    "smoke:local": "node ./scripts/smoke-local.js",
    "smoke:webhook": "node ./scripts/smoke.js",
    "smoke:trim": "node ./scripts/smoke.js | node ./scripts/trim-output.js",
    "smoke:vectors": "node ./scripts/smoke-vectors.js",
    "smoke:vectors:trim": "node ./scripts/smoke-vectors.js | node ./scripts/trim-output.js",
    "mock:server": "node ./scripts/mock-server.js",
    "autonomy": "node --import tsx ./src/cli/run-autonomy.ts",
    "demo:visual-browser": "node ./scripts/visual-browser-demo/demo.mjs",
    "pdf:render": "node ./scripts/pdf/render-court-pdf.mjs",
    "pdf:render:verified": "node ./scripts/pdf/render-court-pdf-verified.mjs",
    "pdf:append": "node ./scripts/pdf/append-pdf.mjs",
    "ui:server": "node ./ui/server.js",
    "ui:client": "npm --prefix ui/client run dev",
    "ui:build": "npm --prefix ui/client run build",
    "verify:audit": "node ./scripts/verify.js",
    "validate:schemas": "node ./scripts/validate-schemas.mjs",
    "validate:receipts": "node ./scripts/validate-receipts.mjs",
    "deepthink": "node ./deepthink/src/deepthink.mjs",
    "ci:deepthink-gates": "node ./scripts/ci/deepthink-gates.mjs",
    "detect:tiers": "node ./scripts/detect-tiers.mjs --out tier_declaration.json",
    "ci:detect-tiers": "node ./scripts/detect-tiers.mjs",
    "sign:manifest": "node ./scripts/signing/sign-manifest.mjs",
    "sign:run": "node ./scripts/signing/sign-run.mjs",
    "ci:verify-signatures": "node ./scripts/ci/verify-signatures.mjs",
    "monitor:log": "tsx src/cli/run-monitoring.ts log",
    "monitor:weekly": "tsx src/cli/run-monitoring.ts weekly-review",
    "monitor:baselines": "tsx src/cli/run-monitoring.ts update-baselines",
    "monitor:test": "tsx tests/monitoring/severity-classifier.runner.ts",
    "diag:elevenlabs": "node scripts/load-env.mjs scripts/diag-elevenlabs-complete.mjs",
    "automations:schema-snapshot": "node scripts/schema-snapshot-automations.mjs",
    "skills:revoke": "node scripts/skills-revoke-one.mjs",
    "skills:check": "node scripts/skills-check-revoked.mjs",
    "skills:status": "node scripts/skills-status.mjs",
    "check:agents": "node ./tools/check-agents.mjs"
  },
  "dependencies": {
    "express": "^5.2.1",
    "rrule": "^2.8.1",
    "yaml": "^2.8.0",
    "yauzl": "^3.2.0",
    "yazl": "^3.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "ajv": "^8.17.1",
    "markdown-it": "^14.1.0",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.57.0",
    "puppeteer": "^24.37.5",
    "qrcode": "^1.5.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.7.2"
  }
}
