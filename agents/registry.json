    },
    {
      "name": "planner-agent",
      "version": "1.1.3",
      "capabilities": ["plan.generate"]
    },
    {
      "name": "document-intake-agent",
      "version": "1.0.0",
      "capabilities": ["intake.scan"]
    },
    {
      "name": "tax-agent",
      "version": "1.0.0",
